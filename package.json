        "test": "ts-node webpack/scripts/test.ts",
        "dev": "yarn start",
        "start": "ts-node webpack/scripts/start.ts",
        "prebuild": "yarn typechain:compile && yarn graphql:compile && yarn i18n:compile",
        "build": "ts-node webpack/scripts/build.ts",
        "serve": "serve -s build -l 3001",
        "webpack:develop": "ts-node webpack/scripts/develop.ts",
        "graphql:compile": "graphql-codegen --config .graphqlrc",
        "i18n:extract": "lingui extract --locale en-US",
        "i18n:compile": "yarn i18n:extract && lingui compile",
        "i18n:pseudo": "lingui extract --locale pseudo && lingui compile",
        "typechain:compile": "typechain --target ethers-v5 --always-generate-overloads --out-dir src/typechain/ './src/abis/*'",
        "version:tag": "yarn changeset tag",
        "version:get": "grep '\"version\"' package.json | sed 's/.*\"version\": \"\\(.*\\)\".*/\\1/'",
        "version:commit": "git add . && git commit -m \"v$(yarn version:get)\"",
        "version:changeset": "yarn changeset add && yarn changeset version",
        "version:platform": "echo \"WINDOWS USER DETECTED - INITATING SELF DESTRUCTION\"",
        "version:push": "git push --follow-tags",
        "release": "yarn version:changeset && yarn version:platform  && yarn version:commit && yarn version:tag"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
